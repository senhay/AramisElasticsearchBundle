{
    "name": "aramis/elasticsearch-bundle",
    "description": "Aramis ElasticSearch Bundle",
    "keywords": [ "aramis", "elasticsearch", "bundle", "elastica", "indexer" ],
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Aramis i-team",
            "homepage": "https://github.com/ARAMISAUTO/AramisElasticsearchBundle/graphs/contributors"
        }
    ],
    "homepage": "https://github.com/ARAMISAUTO/AramisElasticsearchBundle",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3",
        "symfony/console":          "~2.1",
        "symfony/property-access":  "~2.2",
        "jms/serializer-bundle":    "@stable",
        "nategood/httpful":         "0.2.17",
        "ruflin/elastica":          ">=0.90.10.0, <1.4-dev",
        "oldsound/rabbitmq-bundle": "1.5.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Aramis\\Bundle\\ElasticsearchBundle": ""
        }
    },
    "target-dir": "Aramis/Bundle/ElasticsearchBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:ARAMISAUTO/AramisElasticsearchBundle.git"
        }
    ]
}